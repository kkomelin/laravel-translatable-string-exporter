{
  "name": "kkomelin/laravel-translatable-string-exporter",
  "description": "Translatable String Exporter for Laravel >= 5.4",
  "keywords": ["laravel", "translations", "export", "exporter"],
  "license": "MIT",
  "authors": [{
    "name": "Konstantin Komelin",
    "email": "konstantin.komelin@gmail.com"
  }],
  "require": {
    "php": ">=5.4.0",
    "ext-json": "*",
    "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0.*",
    "illuminate/translation": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0.*",
    "symfony/finder": "^3.2|^4.0"
  },
  "autoload": {
    "psr-4": {
      "KKomelin\\TranslatableStringExporter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KKomelin\\TranslatableStringExporter\\Providers\\ExporterServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^1.0"
  }
}
